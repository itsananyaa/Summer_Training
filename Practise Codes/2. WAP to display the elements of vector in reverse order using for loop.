
#include<iostream>
#include<vector>
using namespace std;

int main()
{
    int n;
    cout << "Enter n: ";
    cin >> n;
    vector<int> dis(n);
    for(auto i = 0; i < dis.size(); i++)
    {
        cin >> dis[i];
    }
    for(auto i = n - 1; i >= 0; i--)
    {
        cout << dis[i] << " ";
    }
    return 0;
}
