#include<iostream>
#include<vector>
#include<algorithm>
using namespace std;

int main()
{
    int n;
    cout << "Enter N: ";
    cin >> n;
    vector<int> vec(n);
    vector<int> vec1(n);
    for(int i = 0; i < vec.size(); i++)
    {
        cin >> vec[i];
    }
    cout << "Vector 1" << endl;
    for(int i = 0; i < vec.size(); i++)
    {
        cout << vec[i] << " ";
    }
    for(int i = 0; i < vec.size(); i++)
    {
        vec1[i] = vec[i];
    }
    cout << endl << "Vector 2: " << endl;
    for(int i = 0; i < vec.size(); i++)
    {
        cout << vec1[i] << " ";
    }

    return 0;
}

