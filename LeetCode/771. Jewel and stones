class Solution {
public:
    int numJewelsInStones(std::string jewels, std::string stones) {
        int s = 0;
        for (int i=0; i< jewels.size(); i++) {
            for (int j=0; j<stones.size(); j++) {
                if (jewels[i]==stones[j]) {
                    s++;
                }
            }
        }
        return s;
    }
};
